<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <key n="100">
    <name>d</name>
    <desc>Canopy</desc>
    <binding>
      <command>nasal</command>
      <script>doors.crew.toggle();</script>
    </binding>
  </key>

  <key n="10">
    <name>Ctrl-j</name>
    <desc>Bomb signal</desc>
    <binding>
      <command>property-assign</command>
      <property>controls/signal</property>
      <value>true</value>
    </binding>
  </key>

</PropertyList>

